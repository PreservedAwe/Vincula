<div class="card text-center vw-100">
  <img src="<%= @current_user.profile_picture %>" class="card-img-top" alt="...">
  <div class="card-body">
    <h5 class="card-title"><%= @current_user.username %></h5>
    <% userLocation = Geocoder.search([@current_user.latitude, @current_user.longitude]) %>
    <p class="card-text">Location:<%= userLocation.first.city %>, <%= userLocation.first.country %></p>
    <a href="#" class="btn btn-primary">Go somewhere</a>
  </div>
</div>
